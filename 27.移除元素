class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        if (nums.size() == 0) return 0;
        int j = 0;
        for (vector<int>::size_type i = 0; i != nums.size(); ++i)
        {
            if (nums[i] != val) nums[j++] = nums[i];
            //else continue;
        }
        return j;
    }
};
